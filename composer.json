{
    "name": "murmurcreative/use-fields",
    "description": "A friendly interface for safely interacting with ACF.",
    "authors": [
        {
            "name": "Ben Bateman",
            "email": "ben@murmurcreative.com"
        }
    ],
    "require": {
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Murmur\\WP\\": "src/"
        }
    },
    "scripts": {
        "check": ["phpcs --standard=phpcs.xml src"],
        "fix": ["phpcbf --standard=phpcs.xml src"]
    }
}
